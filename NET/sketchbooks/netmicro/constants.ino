/**
 * Register Names
 */

#define NULL   0x00
#define PING   0x01
#define STATUS 0x02
#define URL    0x03
#define DATA   0x04


/**
 * Labels
 */

// PING reply
#define PONG 0x50

// STATUSes
#define STARTING 0x60
#define OFFLINE  0x61
#define ONLINE   0x62
#define LOADING  0x63
#define LOADED   0x64
#define FAILED   0x65
#define DEAD     0x66
